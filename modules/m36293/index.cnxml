                        </m:mtd>
                      </m:mtr>
                    </m:mtable>
                    <m:mrow/>
                  </m:mrow>
                </m:mrow>
                <m:annotation encoding="StarMath 5.0">alignl { stack {
 size 12{ Int { {  {x rSup { size 8{2} } }  over  {1 - x rSup { size 8{4} } } }  ital "dx"} = Int { {  {1/4}  over  {1 - x} } }  ital "dx"+ Int { {  {1/4}  over  {1+x} }  ital "dx"} + Int { {  { - 1/2}  over  {1+x rSup { size 8{2} } } } }  ital "dx"={}}  {} # 
= -  {  {1}  over  {4} } "ln" \lline 1 - x \lline + {  {1}  over  {4} } "ln" \lline 1+x \lline  -  {  {1}  over  {2} } "arctan"x+C= {  {1}  over  {4} } "ln" {  { \lline 1+x \lline }  over  { \lline 1 - x \lline } }  -  {  {1}  over  {2} } "arctan"x+C "."  {} 
} } {}</m:annotation>
              </m:semantics>
            </m:math>
          </equation>
