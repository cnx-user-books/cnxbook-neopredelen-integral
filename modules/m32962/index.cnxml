                              <m:mstyle fontstyle="italic">
                                <m:mrow>
                                  <m:mtext>dx</m:mtext>
                                </m:mrow>
                              </m:mstyle>
                              <m:mo stretchy="false">=</m:mo>
                              <m:mrow>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mfrac>
                                  <m:mstyle fontstyle="italic">
                                    <m:mrow>
                                      <m:mtext>dt</m:mtext>
                                    </m:mrow>
                                  </m:mstyle>
                                  <m:mn>3</m:mn>
                                </m:mfrac>
                              </m:mrow>
                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
                        <m:mrow/>
                      </m:mtd>
                    </m:mtr>
                  </m:mtable>
                  <m:mrow/>
                </m:mrow>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0">alignl { stack {
 size 12{1 - 3x=t}  {} # 
 size 12{ ital "dx"= -  {  { ital "dt"}  over  {3} } }  {} 
} } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
        <para id="id1169544681978">се сведува на </para>
        <equation id="id1169546318121">
          <m:math>
            <m:semantics>
              <m:mrow>
                <m:mstyle fontsize="12pt">
                  <m:mrow>
                    <m:mrow>
                      <m:mrow>
                        <m:mo stretchy="false">∫</m:mo>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:msqrt>
                              <m:mrow>
                                <m:mrow>
                                  <m:mn>9</m:mn>
                                  <m:mo stretchy="false">−</m:mo>
                                  <m:mo stretchy="false">(</m:mo>
                                </m:mrow>
                                <m:mrow>
                                  <m:mn>1</m:mn>
                                  <m:mo stretchy="false">−</m:mo>
                                  <m:mn>3x</m:mn>
                                </m:mrow>
                                <m:msup>
                                  <m:mo stretchy="false">)</m:mo>
                                  <m:mstyle fontsize="8pt">
                                    <m:mrow>
                                      <m:mn>2</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:msup>
                              </m:mrow>
                            </m:msqrt>
                          </m:mfrac>
                          <m:mrow>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
                                <m:mtext>dx</m:mtext>
                              </m:mrow>
                            </m:mstyle>
                            <m:mo stretchy="false">=</m:mo>
                            <m:mrow>
                              <m:mo stretchy="false">−</m:mo>
                              <m:mfrac>
                                <m:mn>1</m:mn>
                                <m:mn>3</m:mn>
                              </m:mfrac>
                            </m:mrow>
                          </m:mrow>
                        </m:mrow>
                      </m:mrow>
                      <m:mrow>
                        <m:mo stretchy="false">∫</m:mo>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:msqrt>
                              <m:mrow>
                                <m:mn>9</m:mn>
                                <m:mo stretchy="false">−</m:mo>
                                <m:msup>
                                  <m:mi>t</m:mi>
                                  <m:mstyle fontsize="8pt">
                                    <m:mrow>
                                      <m:mn>2</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:msup>
                              </m:mrow>
                            </m:msqrt>
                          </m:mfrac>
                          <m:mrow>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
                                <m:mtext>dt</m:mtext>
                              </m:mrow>
                            </m:mstyle>
                            <m:mo stretchy="false">=</m:mo>
                            <m:mrow>
                              <m:mo stretchy="false">−</m:mo>
                              <m:mfrac>
                                <m:mn>1</m:mn>
                                <m:mn>3</m:mn>
                              </m:mfrac>
                            </m:mrow>
                          </m:mrow>
                        </m:mrow>
                      </m:mrow>
                      <m:mtext>arcsin</m:mtext>
                      <m:mrow>
                        <m:mrow>
                          <m:mfrac>
                            <m:mi>t</m:mi>
                            <m:mn>3</m:mn>
                          </m:mfrac>
                          <m:mo stretchy="false">+</m:mo>
                          <m:mi>C</m:mi>
                        </m:mrow>
                        <m:mo stretchy="false">=</m:mo>
                        <m:mrow>
                          <m:mo stretchy="false">−</m:mo>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mn>3</m:mn>
                          </m:mfrac>
                        </m:mrow>
                      </m:mrow>
                      <m:mtext>arcsin</m:mtext>
                      <m:mrow>
                        <m:mfrac>
                          <m:mrow>
                            <m:mn>1</m:mn>
                            <m:mo stretchy="false">−</m:mo>
                            <m:mn>3x</m:mn>
                          </m:mrow>
                          <m:mn>3</m:mn>
                        </m:mfrac>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mi>C</m:mi>
                      </m:mrow>
                      <m:mtext>.</m:mtext>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ Int { {  {1}  over  { sqrt {9 -  \( 1 - 3x \)  rSup { size 8{2} } } } }  ital "dx"= -  {  {1}  over  {3} } }  Int { {  {1}  over  { sqrt {9 - t rSup { size 8{2} } } } }  ital "dt"= -  {  {1}  over  {3} } } "arcsin" {  {t}  over  {3} } +C= -  {  {1}  over  {3} } "arcsin" {  {1 - 3x}  over  {3} } +C "." } {}</m:annotation>
            </m:semantics>
